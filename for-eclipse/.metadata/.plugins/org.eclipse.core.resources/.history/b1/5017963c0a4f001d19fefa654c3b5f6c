package com.xworkz.employeeapp;

public class IronBox {
		
		public static void main(String args[]) {
			
			IronBox ir = new IronBox("USHA123","USHA","PIGiron");
			System.out.println("IrionBox details");
			System.out.println(ir.ironId +" "+ir.name + " "+ir.type);
			
			IronBox ir1 = new IronBox("USHA345","philips","casteiron");
			System.out.println("IrionBox details");
			System.out.println(ir1.ironId +" "+ir1.name + " "+ir1.type);
			
			
			IronBox ir2 = new IronBox("USHA789","USHA","pureiron");
			System.out.println("IrionBox details");
			System.out.println(ir2.ironId +" "+ir2.name + " "+ir2.type);
		}

	}


}
