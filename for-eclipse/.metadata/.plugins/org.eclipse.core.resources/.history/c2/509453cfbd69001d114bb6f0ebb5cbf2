package com.xworkz.encapsulationapp;

public class AnimalDTO {
	private  String name;
	private  String colour;
	
	public  void setName(String nm) {
		this.name=nm;
	}
	
	public String getName() {
		return name;
	}
	
	public void setcolour(String clr) {
		this.colour=clr;
	}
	public String getColour() {
		return colour;
	}
	

}




