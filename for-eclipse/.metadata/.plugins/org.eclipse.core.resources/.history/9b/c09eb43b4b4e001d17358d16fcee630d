package com.xworkz.Airportapp;

public class FootWareTester {
	public static void main(String args[]) {
		 FootWare ftw = new FootWare("id1","pink",100,"lunars",4,"sandal");
		 System.out.println("FootWare Details");
		 System.out.println(ftw.footWareId + " "+ fwt.color +" "+ fwt.price + " "+fwt.brandName + " "+fwt.size + " "+fwt.model);
		 
		 FootWare ftw1 = new FootWare("id2","black",4000,"bata",5,"hills");
		 System.out.println("FootWare Details");
		 System.out.println(ftw1.footWareId + " "+ ftw1.color +" "+ ftw1.price + " "+ftw1.brandName + " "+fwt1.size + " "+fwt1.model);
		 
		 FootWare ftw2 = new FootWare("id3","brown",600,"walkmate",6,"flat");
		 System.out.println("FootWare Details");
		 System.out.println(ftw2.footWareId + " "+ fwt2.color +" "+ fwt2.price + " "+fwt2.brandName + " "+fwt2.size + " "+fwt2.model);
		 
	}
}
