package com.xworkz.employeeapp;

public class WatchTester {

	
		public static void main(String args[]) {
			 Watch wt = new Watch("Q123","pink",1000,"QUARTZ","lather");
			 System.out.println("Watch Details");
			 System.out.println(wt.watchId + " "+ wt.color +" "+ wt.price + " "+wt.brandName + " "+wt.type);
			 
			 Watch wt1 = new Watch("Q345","black",500,"boat","lather");
			 System.out.println("Watch Details");
			 System.out.println(wt1.watchId + " "+ wt1.color +" "+ wt1.price + " "+wt1.brandName + " "+wt1.type);
			 
			 
			 Watch wt2 = new Watch("Q788","brown",2000,"noise","lather");
			 System.out.println("Watch Details");
			 System.out.println(wt2.watchId + " "+ wt2.color +" "+ wt2.price + " "+wt2.brandName + " "+wt2.type);
			 
			 


		}

	}



	
