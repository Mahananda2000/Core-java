package com.xworkz.encapsulationapp;

public class AnimalDTO {
	private  String name;
	private  String colour;
	
	void setName(String nm) {
		this.name=nm;
	}
	
	String getName() {
		return name;
	}
	
	void setcolour(String clr) {
		this.colour=clr;
	}
	String getColour() {
		return colour;
	}
	

}




