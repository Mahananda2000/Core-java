public class Palindrome {
	public static void main(String args[]) {
		int num=1234;
		int rev=0;
		int digit;
		int temp=num;
		while(num!=0) {
			digit=num%10;
			rev=(rev*10)+digit;
			num=num/10;
		
			
		}
		if(temp==num) {
			
		}
			System.out.println(rev);	
		}

}
